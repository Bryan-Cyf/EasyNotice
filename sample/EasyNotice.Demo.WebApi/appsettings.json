{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Notice": {
    "IntervalSeconds": 60, //发送间隔（仅限同一标题），避免短时间内发送大量相同的消息

    //【邮件配置】
    "Mail": {
      "Host": "smtp.qq.com", //SMTP地址
      "Port": 465, //SMTP端口
      "FromName": "System", //发件人（可自定义）
      "FromAddress": "12345@qq.com", //发件地址（需与密码匹配）
      "Password": "password", //发件密码
      "ToAddress": [ "a@qq.com" ] //接收邮件的地址
    },

    //【钉钉群机器人配置】
    "DingTalk": {
      "WebHook": "https://oapi.dingtalk.com/robot/send?access_token=xxxxx", //通知回调的钩子
      "Secret": "secret" //签名校验
    },

    //【飞书群机器人配置】
    "FeiShu": {
      "WebHook": "https://open.feishu.cn/open-apis/bot/v2/xxxxx", //通知回调的钩子
      "Secret": "secret" //签名校验
    }
  }
}
